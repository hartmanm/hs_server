/learn_you_a_haskell    <html><body></><a href='https://learnyouahaskell.com/starting-out#im-a-list-comprehension'>https://learnyouahaskell.com/starting-out#im-a-list-comprehension</a></body></html>

/btc                    <html><head><style>pre{background-color:#f9f9f9;padding:12px;overflow:auto;white-space:pre-wrap;}</style></head><body><div id="output"></div><script>fetch('https://api.allorigins.win/get?url='+encodeURIComponent('https://api.coingecko.com/api/v3/simple/price?ids=bitcoin&vs_currencies=usd')).then(response=>response.json()).then(data=>{var responseData=JSON.parse(data.contents);var formattedJson=JSON.stringify(responseData,null,2);document.getElementById('output').innerHTML='<pre>'+escapeHtml(formattedJson)+'</pre>';}).catch(error=>{document.getElementById('output').textContent='Error';console.error(error);});function escapeHtml(html){return html.replace(/&/g,"&amp;").replace(/</g,"&lt;").replace(/>/g,"&gt;").replace(/"/g,"&quot;").replace(/'/g,"&#39;");}</script></body></html>

/coins                  <html><head><style>pre{background-color:#f9f9f9;padding:12px;overflow:auto;white-space:pre-wrap;}</style></head><body><div id="output"></div><script>fetch('https://api.allorigins.win/get?url='+encodeURIComponent('https://api.coingecko.com/api/v3/coins/markets?vs_currency=usd&order=market_cap_desc&per_page=10&page=1')).then(function(response){return response.json();}).then(function(data){var responseData=JSON.parse(data.contents);var formattedJson=JSON.stringify(responseData,null,2);document.getElementById('output').innerHTML='<pre>'+escapeHtml(formattedJson)+'</pre>';}).catch(function(error){document.getElementById('output').textContent='Error';console.error(error);});function escapeHtml(html){return html.replace(/&/g,"&amp;").replace(/</g,"&lt;").replace(/>/g,"&gt;").replace(/"/g,"&quot;").replace(/'/g,"&#39;");}</script></body></html>

